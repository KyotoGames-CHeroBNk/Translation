translate rus day1_50790045:

    # tel_mm "{nw}Hej Monika"
    # tel_mm "{nw}Hey Monika"
    tel_mm "{nw}Привет, Моника"

# game/day1.rpy:1851
translate rus day1_28c9a2c3:

    # tel_mm "{nw}Córeczko jak ci mija drugi dzień szkoły?"
    # tel_mm "{nw}Daughter how is your second day of school going?"
    tel_mm "{nw}Дорогая, как проходит твой второй день в школе?"

# game/day1.rpy:1853
translate rus day1_575400fe:

    # tel_mm "{nw}Monika?"
    # tel_mm "{nw}Monika?"
    tel_mm "{nw}Моника?"

# game/day1.rpy:1854
